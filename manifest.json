{
  "manifest_version": 2,
  "name": "LeetCode result expander",
  "version": "1.0",
  "description": "Expands LeetCode input, output and expected containers to show all values.",
  "background": {
    "scripts": ["js/background.js"] 
  },
  "permissions": [
    "tabs","https://leetcode.com/problems/*"
  ],
    "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/32_off.png"
  }
}