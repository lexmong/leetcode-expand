{
  "manifest_version": 3,
  "name": "LeetCode expand",
  "version": "2.1",
  "description": "Expands LeetCode input, output and expected containers to show all values.",
  "content_scripts": [
   {
    "matches": ["https://leetcode.com/problems/*"],
    "exclude_matches": ["*://*/*/all/"],
    "css": ["css/styles.css"],
    "js": ["js/content.js"]
   }
 ],
    "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
